# Staging Environment Variables for Terraform
# This file contains infrastructure-specific variables for staging deployment
# Usage: source infrastructure/.env.staging before running terraform commands

# Terraform Variables for Infrastructure Deployment
export TF_VAR_sql_admin_username=sqladmin
export TF_VAR_sql_admin_password="SecurePass123!"
export TF_VAR_developer_ip=  # Leave empty to skip developer IP access

# Environment Configuration
export TF_VAR_environment=staging
export TF_VAR_location="francecentral"

# Resource Sizing for Staging (cost-optimized)
# These will be used by the tfvars file or can override defaults
export TF_VAR_sql_sku=Basic
export TF_VAR_app_service_plan_sku=Y1
